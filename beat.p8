end

// draw sprites
function _draw()
 cls()
 map(0,0)
 spr(p.sp,p.x,p.y) --player
 
 spr(7,p.x-6,p.y)
 spr(7,p.x+6,p.y)
 spr(7,p.x,p.y-6)
 spr(7,p.x,p.y+6)
end

